				token('Symbol')
			end
		elseif c1 == '.' then
			-- Greedily consume up to 3 `.` for . / .. / ... tokens
			-- Also consume "..=" compound concatenation operator in this case
			if look() == '.' then
				get()
				if look() == '.' or look() == '=' then
