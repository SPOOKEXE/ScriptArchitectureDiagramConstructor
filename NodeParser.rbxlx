				end
			else
				-- Normal Number
				while DigitsPlusSeparator[look()] do
					p = p + 1
				end
				if look() == '.' then
					-- With decimal point
					p = p + 1
					while DigitsPlusSeparator[look()] do
						p = p + 1
					end
				end
				if look() == 'e' or look() == 'E' then
					-- With exponent
					p = p + 1
					if look() == '-' then
						p = p + 1
					end
					while DigitsPlusSeparator[look()] do
