-- Adds / removes whitespace in an AST to put it into a "standard formatting"
local function formatAst(ast)
	local formatStat
	local formatExpr
	local formatType

	local currentIndent = 0

	local function applyIndent(token)
		local indentString = '\n'..('\t'):rep(currentIndent)
